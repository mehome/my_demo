	depends on HTTPD

config ZPAGE_GZIP
	bool "Compress webpages with gzip"
	depends on HTTPD && ZPAGE

config WEB_AS_DATA
	bool "Web pages as data"
	depends on HTTPD

config MULTI_LOGIN
	bool "Allow multiple login"
	depends on HTTPD

