	depends on HTTPS

config WWW
	tristate "WWW"
	default y

choice
	prompt "Compress HTML"
	default COMPRESS_HTML_BY_HTMLMINIFIER
	depends on WWW
	help
	  select tool to compress javascript

	config COMPRESS_HTML_BY_NONE
		bool "None"

	config COMPRESS_HTML_BY_HTMLMINIFIER
		bool "html-minifier"

	config COMPRESS_HTML_BY_PYTHONSLIMMER
		bool "PythonHTMLSlimmer"
endchoice

choice
	prompt "Compress javascript"
	default COMPRESS_JS_BY_UGLIFYJS
	depends on WWW
	help
	  select tool to compress javascript

	config COMPRESS_JS_BY_NONE
		bool "None"

	config COMPRESS_JS_BY_UGLIFYJS
		bool "uglify-js"

	config COMPRESS_JS_BY_YUICOMPRESSOR
		bool "YUI-compressor"
endchoice

choice
	prompt "Compress CSS"
	default COMPRESS_CSS_BY_YUICOMPRESSOR
	depends on WWW
	help
	  select tool to compress CSS

	config COMPRESS_CSS_BY_NONE
		bool "None"

	config COMPRESS_CSS_BY_YUICOMPRESSOR
		bool "YUI-compressor"
endchoice

config WIZARD_WEB
	bool "Enable wizard subpage"
	depends on WWW

config FILE_EXPLORER
	bool "Enable file explorer page"
	depends on WWW
	default y

config DHCPS_PROBE
