LIBRARYS += -lhttp_json -lhttp_fifo -lhttp_ret_json -lhttp_sem -lhttp_uci

MUSICLISTPLAY_TARGET = postmusiclistplay.cgi
#推送并播放列表，需要解析JSON信息

MUSICLISTPLAYEX_TARGET = postmusiclistplayex.cgi
#推送并播放列表扩展，需要解析JSON信息

MUSICLISTINSERT_TARGET = postinsertsomeplaylistinfo.cgi
#推送批量插入音乐列表，需要解析JSON信息

SETALARMCLOCK_TARGET = postsetalarmclock.cgi
#推送设置定时器的信息，需要解析JSON信息

SYNCHRONOUSINFOEX_TARGET = postsynchronousinfoex.cgi
#推送同步信息扩展，JSON信息现在直接放在后台去执行，这边不做JSON解析

INSERTPLAYLISTINFO_TARGET = postinsertplaylistinfo.cgi
#推送插入当前播放列表信息，需要解析JSON信息

BLUETOOTHUPDATE_TARGET = postbluetoothupdate.cgi
#推送蓝牙更新固件，需要解析JSON信息

CONEXANTSFS_TARGET = PostConexantSfs.cgi
#推送Conexant更新固件所需要的sfs文件，需要解析JSON信息

CONEXANTBIN_TARGET = PostConexantBin.cgi
#推送Conexant更新固件所需要的bin文件，需要解析JSON信息

REMOTEBLUETOOTHUPDATE_TARGET = RemoteBluetoothUpdate.cgi
#推送蓝牙升级URL，需要解析JSON信息

CREATEALARMCLOCK_TARGET = CreateAlarmClock.cgi
#创建闹钟，需要解析JSON信息

CHANGEALARMCLOCKPLAY_TARGET = ChangeAlarmClockPlay.cgi
#修改闹钟，需要解析JSON信息

COUNTDOWNALARMCLOCK_TARGET = CountdownAlarmClock.cgi
#倒计时闹钟，需要解析JSON信息

PLAYALBUMASPLAN_TARGET = PlayAlbumAsPlan.cgi
#定时播放专辑，需要解析JSON信息

PLAYMUSICLISTASPLAN_TARGET = PlayMusicListAsPlan.cgi
#定时播放列表，需要解析JSON信息

PLAYMIXEDLISTASPLAN_TARGET = PlayMixedListAsPlan.cgi
#定时混合播放列表，需要解析JSON信息

SHORTCUTKEY_TARGET = CreateShortcutKeyList.cgi
#快捷键播放列表，需要解析JSON信息


all:	$(MUSICLISTPLAY_TARGET) $(MUSICLISTPLAYEX_TARGET) $(MUSICLISTINSERT_TARGET) \
		$(SETALARMCLOCK_TARGET)  $(SYNCHRONOUSINFOEX_TARGET) $(INSERTPLAYLISTINFO_TARGET) \
		$(BLUETOOTHUPDATE_TARGET) \
		$(CONEXANTSFS_TARGET) $(CONEXANTBIN_TARGET) $(REMOTEBLUETOOTHUPDATE_TARGET) \
		$(CREATEALARMCLOCK_TARGET) $(CHANGEALARMCLOCKPLAY_TARGET) $(COUNTDOWNALARMCLOCK_TARGET) \
		$(PLAYALBUMASPLAN_TARGET) $(PLAYMUSICLISTASPLAN_TARGET) $(PLAYMIXEDLISTASPLAN_TARGET) \
		$(SHORTCUTKEY_TARGET)
	
$(MUSICLISTPLAY_TARGET):	$(MUSICLISTPLAY_TARGET:%.cgi=%.c)
	$(CC) $(@:%.cgi=%.c) -o $@ $(CFLAGS) $(LDFLAGS) $(LIBRARYS)
	
$(MUSICLISTPLAYEX_TARGET):	$(MUSICLISTPLAYEX_TARGET:%.cgi=%.c)  
	$(CC) $(@:%.cgi=%.c) -o $@ $(CFLAGS) $(LDFLAGS) $(LIBRARYS)
	
$(MUSICLISTINSERT_TARGET):	$(MUSICLISTINSERT_TARGET:%.cgi=%.c)  
	$(CC) $(@:%.cgi=%.c) -o $@ $(CFLAGS) $(LDFLAGS) $(LIBRARYS)

$(SETALARMCLOCK_TARGET):	$(SETALARMCLOCK_TARGET:%.cgi=%.c)  
	$(CC) $(@:%.cgi=%.c) -o $@ $(CFLAGS) $(LDFLAGS) $(LIBRARYS)
	
$(SYNCHRONOUSINFOEX_TARGET):	$(SYNCHRONOUSINFOEX_TARGET:%.cgi=%.c)  
	$(CC) $(@:%.cgi=%.c) -o $@ $(CFLAGS) $(LDFLAGS) $(LIBRARYS)
	
$(INSERTPLAYLISTINFO_TARGET):	$(INSERTPLAYLISTINFO_TARGET:%.cgi=%.c)  
	$(CC) $(@:%.cgi=%.c) -o $@ $(CFLAGS) $(LDFLAGS) $(LIBRARYS)
	
$(BLUETOOTHUPDATE_TARGET):	$(BLUETOOTHUPDATE_TARGET:%.cgi=%.c)  
	$(CC) $(@:%.cgi=%.c) -o $@ $(CFLAGS) $(LDFLAGS) $(LIBRARYS)
	
$(CONEXANTSFS_TARGET):	$(CONEXANTSFS_TARGET:%.cgi=%.c)  
	$(CC) $(@:%.cgi=%.c) -o $@ $(CFLAGS) $(LDFLAGS) $(LIBRARYS)
	
$(CONEXANTBIN_TARGET):	$(CONEXANTBIN_TARGET:%.cgi=%.c)  
	$(CC) $(@:%.cgi=%.c) -o $@ $(CFLAGS) $(LDFLAGS) $(LIBRARYS)

$(REMOTEBLUETOOTHUPDATE_TARGET):	$(REMOTEBLUETOOTHUPDATE_TARGET:%.cgi=%.c)  
	$(CC) $(@:%.cgi=%.c) -o $@ $(CFLAGS) $(LDFLAGS) $(LIBRARYS)

$(CREATEALARMCLOCK_TARGET):	$(CREATEALARMCLOCK_TARGET:%.cgi=%.c)  
	$(CC) $(@:%.cgi=%.c) -o $@ $(CFLAGS) $(LDFLAGS) $(LIBRARYS)
	
$(CHANGEALARMCLOCKPLAY_TARGET):	$(CHANGEALARMCLOCKPLAY_TARGET:%.cgi=%.c)  
	$(CC) $(@:%.cgi=%.c) -o $@ $(CFLAGS) $(LDFLAGS) $(LIBRARYS)
	
$(COUNTDOWNALARMCLOCK_TARGET):	$(COUNTDOWNALARMCLOCK_TARGET:%.cgi=%.c)  
	$(CC) $(@:%.cgi=%.c) -o $@ $(CFLAGS) $(LDFLAGS) $(LIBRARYS)
	
$(PLAYALBUMASPLAN_TARGET):	$(PLAYALBUMASPLAN_TARGET:%.cgi=%.c)  
	$(CC) $(@:%.cgi=%.c) -o $@ $(CFLAGS) $(LDFLAGS) $(LIBRARYS)
	
$(PLAYMUSICLISTASPLAN_TARGET):	$(PLAYMUSICLISTASPLAN_TARGET:%.cgi=%.c)  
	$(CC) $(@:%.cgi=%.c) -o $@ $(CFLAGS) $(LDFLAGS) $(LIBRARYS)
	
$(PLAYMIXEDLISTASPLAN_TARGET):	$(PLAYMIXEDLISTASPLAN_TARGET:%.cgi=%.c)  
	$(CC) $(@:%.cgi=%.c) -o $@ $(CFLAGS) $(LDFLAGS) $(LIBRARYS)
	
$(SHORTCUTKEY_TARGET):	$(SHORTCUTKEY_TARGET:%.cgi=%.c)  
	$(CC) $(@:%.cgi=%.c) -o $@ $(CFLAGS) $(LDFLAGS) $(LIBRARYS)
	
.PHONY:	clean
clean:
	rm -rf \
		$(MUSICLISTPLAY_TARGET) $(MUSICLISTPLAYEX_TARGET) $(MUSICLISTINSERT_TARGET) \
		$(SETALARMCLOCK_TARGET)  $(SYNCHRONOUSINFOEX_TARGET) $(INSERTPLAYLISTINFO_TARGET) \
		$(BLUETOOTHUPDATE_TARGET) \
		$(CONEXANTSFS_TARGET) $(CONEXANTBIN_TARGET) $(REMOTEBLUETOOTHUPDATE_TARGET) \
		$(CREATEALARMCLOCK_TARGET) $(CHANGEALARMCLOCKPLAY_TARGET) $(COUNTDOWNALARMCLOCK_TARGET) \
		$(PLAYALBUMASPLAN_TARGET) $(PLAYMUSICLISTASPLAN_TARGET) $(PLAYMIXEDLISTASPLAN_TARGET) \
		$(SHORTCUTKEY_TARGET)
